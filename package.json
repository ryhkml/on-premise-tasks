{
	"name": "op-tasks",
	"version": "1.0.50",
	"private": true,
	"maintainers": [
		"Reyhan Kamil"
	],
	"scripts": {
		"bin": "bun build --compile --minify --sourcemap src/main.ts --outfile ./tasks",
		"dev": "NODE_ENV=development bun run --hot src/main.ts",
		"test": "bun run reinit:testdb && bun --env-file=.env.development test --timeout 10000",
		"reinit:db": "NODE_ENV=production bun run init-db.ts",
		"reinit:testdb": "NODE_ENV=development bun run init-db.ts"
	},
	"dependencies": {
		"@elysiajs/eden": "^1.0.11",
		"crypto-js": "^4.2.0",
		"date-fns": "^3.6.0",
		"elysia": "^1.0.13",
		"lodash": "^4.17.21",
		"nanoid": "^5.0.6",
		"rxjs": "^7.8.1",
		"ulid": "^2.3.0"
	},
	"devDependencies": {
		"@types/crypto-js": "^4.2.2",
		"@types/lodash": "^4.17.0",
		"bun-types": "latest"
	},
	"module": "src/main.ts"
}